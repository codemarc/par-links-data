{
  "name": "parlinks",
  "version": "0.0.1",
  "description": "Boilerplate code for building web extensions using parcel",
  "author": "Marc J. Greenberg <Marc_Greenberg@partech.com>",
  "license": "ISC",
  "scripts": {
    "start": "parcel manifest.json --host localhost --target webext-dev",
    "build": "parcel build manifest.json --target webext-prod"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/config-webextension": "2.0.0-nightly.2312",
    "parcel": "2.0.0-beta.3.1"
  },
  "targets": {
    "webext-dev": {
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "webext-prod": {}
  }
}
